{
  "name": "quortex",
  "version": "1.0.0",
  "description": "A web-based implementation of Quortex game.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "cd server && npm run dev",
    "build": "tsc && vite build",
    "build:multiplayer": "tsc && vite build --config vite.multiplayer.config.ts",
    "build:server": "cd server && npm run build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "postinstall": "node scripts/patch-playwright.cjs && cd server && npm install",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@redux-devtools/extension": "^3.3.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@discord/embedded-app-sdk": "^2.4.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "redux": "^5.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "svelte": "^5.43.6",
    "uuid": "^13.0.0"
  }
}
